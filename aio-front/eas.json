{
	"cli": {
		"version": ">= 3.16.0"
	},
	"build": {
		"development": {
			"distribution": "internal",
			"android": {
				"gradleCommand": ":app:assembleDebug"
			},
			"ios": {
				"buildConfiguration": "Debug"
			}
		},
		"preview": {
			"extends": "production",
			"android": {
				"gradleCommand": ":app:assembleRelease"
			},
			"distribution": "internal"
		},
		"production": {
			"autoIncrement": true
		}
	},
	"submit": {
		"production": {
			"ios": {
				"appName": "All In One - AIO"
			}
		}
	}
}
